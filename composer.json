{
    "name": "slim/teste",
    "description": "A Slim Framework skeleton application for rapid development",
    "require": {
        "slim/slim": "^3.12",
        "monolog/monolog": "^1.25",
        "illuminate/database": "~5.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}